{
  "name": "starshell-extension-wallet",
  "displayName": "StarShell Wallet Beta",
  "private": true,
  "type": "module",
  "version": "0.6.0",
  "description": "A privacy-preserving, free and open-source Web3 wallet built for the Secret Network and broader Cosmos ecosystem",
  "license": "MIT",
  "author": {
    "name": "Blake Regalia",
    "email": "blake@starshell.net",
    "url": "https://blake-regalia.net/"
  },
  "contributors": [],
  "eslintIgnore": [
    "dist",
    "node_modules",
    "submodules",
    ".eslintrc.cjs",
    "comments.ts",
    "vite.config.ts"
  ],
  "scripts": {
    "lint": "eslint --ext .ts,.svelte .",
    "build": "scripty",
    "build:chrome": "SCRIPTY_PARALLEL=true scripty",
    "build:firefox": "SCRIPTY_PARALLEL=true scripty",
    "build:safari": "SCRIPTY_PARALLEL=true scripty",
    "build:ios": "SCRIPTY_PARALLEL=true scripty",
    "watch:chrome": "ENGINE=chrome vite build --mode development --watch",
    "watch:firefox": "ENGINE=firefox vite build --mode development --watch",
    "watch:safari": "ENGINE=safari vite build --mode development --watch",
    "watch:ios": "ENGINE=ios vite build --mode development --watch",
    "dev:chrome": "yarn watch:chrome",
    "dev:firefox": "ENGINE=firefox vite",
    "dev:safari": "yarn watch:safari",
    "serve:chrome": "ENGINE=chrome web-ext run -t chromium --start-url \"https://google.com\" --source-dir ./dist/chrome",
    "serve:firefox": "scripty",
    "serve:android-firefox": "scripty",
    "deploy:android-kiwi-webkit": "yarn build:safari && adb push dist/safari.zip /sdcard/Download",
    "deploy:android-kiwi-chromium": "yarn build:chrome && adb -e push dist/chrome.zip /sdcard/Download",
    "host:page": "sirv local --dev --port=8188",
    "host:chrome": "sirv dist/chrome --dev --port=8128",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@cosmjs/amino": "^0.29.3",
    "@cosmjs/launchpad": "^0.27.1",
    "@cosmjs/proto-signing": "^0.29.3",
    "@keplr-wallet/provider": "^0.11.1",
    "@keplr-wallet/types": "^0.10.24",
    "@keplr-wallet/wc-client": "^0.10.24",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.49",
    "@tsconfig/svelte": "^3.0.0",
    "@types/chrome": "^0.0.200",
    "@types/crypto-js": "^4.1.1",
    "@types/estree": "^1.0.0",
    "@types/libsodium-wrappers": "^0.7.10",
    "@types/ua-parser-js": "^0.7.36",
    "@types/web": "^0.0.79",
    "@types/webextension-polyfill": "^0.9.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "acorn-walk": "^8.2.0",
    "autoprefixer": "^10.4.13",
    "crypto-js": "^4.1.1",
    "esbuild": "^0.15.13",
    "eslint": "^8.27.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-i": "^2.26.0-2",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "less": "^4.1.3",
    "postcss": "^8.4.19",
    "rollup": "^2.78.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-graph": "^2.0.0",
    "scripty": "^2.1.1",
    "secretjs": "^1.4.5",
    "sirv": "^2.0.2",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "vite": "^2.9.12",
    "vite-plugin-singlefile": "^0.13.1",
    "web-ext": "^7.3.1",
    "webextension-polyfill": "^0.10.0"
  },
  "dependencies": {
    "@cosmjs/encoding": "^0.29.3",
    "@grpc/grpc-js": "^1.7.3",
