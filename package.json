{
  "name": "starshell-extension-wallet",
  "displayName": "StarShell Wallet Beta",
  "private": true,
  "type": "module",
  "version": "0.2.2",
  "description": "A privacy-preserving, free and open-source Web3 wallet built for the Secret Network and broader Cosmos ecosystem",
  "license": "MIT",
  "author": {
    "name": "Blake Regalia",
    "email": "blake@starshell.net",
    "url": "https://blake-regalia.net/"
  },
  "contributors": [],
  "eslintIgnore": [
    "dist",
    "node_modules",
    "submodules",
    ".eslintrc.cjs",
    "comments.ts",
    "vite.config.ts"
  ],
  "scripts": {
    "lint": "eslint --ext .ts,.svelte .",
    "build": "yarn build:chrome",
    "build:chrome": "PLATFORM=chrome vite build && rm -f dist/chrome.zip && zip -r dist/chrome.zip dist/chrome",
    "build:firefox": "PLATFORM=firefox vite build && rm -f dist/firefox.zip && cd dist/firefox && zip -r ../firefox.zip ./*",
    "build:firefox-android": "PLATFORM=firefox-android vite build && rm -f dist/firefox-android.zip && cd dist/firefox-android && zip -r ../firefox-android.zip ./*",
    "build:safari": "PLATFORM=safari vite build && rm -f dist/safari.zip && zip -r dist/safari.zip dist/safari",
    "watch": "yarn watch:chrome",
    "watch:chrome": "PLATFORM=chrome vite build --watch",
    "watch:firefox": "PLATFORM=firefox vite build --watch",
    "watch:firefox-android": "PLATFORM=firefox-android vite build --watch",
    "watch:safari": "PLATFORM=safari vite build --watch",
    "dev": "yarn dev:firefox",
    "dev:chrome": "yarn watch:chrome",
    "dev:firefox": "PLATFORM=firefox vite",
    "dev:firefox-android": "PLATFORM=firefox-android vite",
    "dev:safari": "yarn watch:safari",
    "serve:chrome": "PLATFORM=chrome web-ext run -t chromium --start-url \"https://google.com\" --source-dir ./dist/chrome",
    "serve:firefox": "PLATFORM=firefox web-ext run --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/firefox",
    "serve:firefox-android": "PLATFORM=firefox-android web-ext run -t firefox-android --android-device=emulator-5554 --adb-remove-old-artifacts --firefox-apk=org.mozilla.firefox_beta --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/firefox-android",
    "deploy:chromium-kiwi": "yarn build:safari && adb push dist/safari.zip /sdcard/Download",
    "host:page": "sirv local --dev --port=8188",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@keplr-wallet/types": "^0.10.19",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@solar-republic/vite-plugin-web-extension": "^1.0.2",
    "@sveltejs/vite-plugin-svelte": "^1.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/chrome": "^0.0.195",
    "@types/crypto-js": "^4.1.1",
    "@types/estree": "^1.0.0",
    "@types/ua-parser-js": "^0.7.36",
    "@types/webextension-polyfill": "^0.9.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "acorn-walk": "^8.2.0",
    "crypto-js": "^4.1.1",
    "esbuild": "^0.15.5",
    "eslint": "^8.23.0",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "less": "^4.1.3",
    "rollup": "^2.78.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "sirv": "^2.0.2",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2",
    "vite": "^2.9.12",
    "web-ext": "^7.1.1",
    "webextension-polyfill": "^0.10.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.8",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@keplr-wallet/provider": "^0.10.19",
    "@solar-republic/cosmos-grpc": "^0.1.1",
    "@solar-republic/svelte-toggle": "^3.1.0",
    "@solar-republic/wasm-secp256k1": "^0.0.3-beta",
    "@zerodevx/svelte-json-view": "^0.2.1",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.0.2",
    "deepmerge": "^4.2.2",
    "fuse.js": "^6.6.2",
    "grpc-web": "^1.3.1",
    "html5-qrcode": "^2.2.1",
    "javascript-time-ago": "^2.5.7",
    "long": "^4.0.0",
    "qrcode-svg": "^1.1.0",
    "semver": "^7.3.7",
    "sha256-uint8array": "^0.10.3",
    "svelte-select": "^4.4.7",
    "svelte-tabs": "^1.1.0",
    "ua-parser-js": "^1.0.2"
  }
}
