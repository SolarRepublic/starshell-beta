    "bignumber.js": "^9.1.0",
    "deepmerge": "^4.2.2",
    "fuse.js": "^6.6.2",
    "grpc-web": "^1.4.2",
    "html5-qrcode": "^2.2.8",
    "javascript-time-ago": "^2.5.9",
    "libsodium-wrappers": "^0.7.10",
    "long": "^4.0.0",
    "miscreant": "^0.3.2",
    "qrcode-svg": "^1.1.0",
    "semver": "^7.3.8",
    "sha256-uint8array": "^0.10.3",
    "showdown": "^2.1.0",
    "svelte-select": "^4.4.7",
    "svelte-tabs": "^1.1.0",
    "toml": "^3.0.0",
    "ua-parser-js": "^1.0.32"
  }
}
